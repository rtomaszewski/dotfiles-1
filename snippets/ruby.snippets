snippet seqmiglong
abbr "Sequel migration up/down"
    Sequel.migration do
      up do
        ${1:Up migration}
      end
      down do
        ${2:Down migration} 
      end
    end

snippet seqmig
abbr "Sequel migration change"
    Sequel.migration do
      change do
        ${1}
      end
    end

snippet apg
abbr "awesome print"
    ap [${1:pg.uri.to_s}, $2]
